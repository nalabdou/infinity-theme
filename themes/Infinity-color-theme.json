	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0088ff"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#f05f85"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"foreground": "#fcc80b"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#f53431"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, storage.type.class.js",
			"settings": {
				"foreground": "#da8805"
			}
		},
		{
			"name": "Meta",
